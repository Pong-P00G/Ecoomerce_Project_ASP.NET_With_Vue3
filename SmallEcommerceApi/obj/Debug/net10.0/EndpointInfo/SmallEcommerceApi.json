          "username": {
            "maxLength": 100,
            "type": "string"
          },
          "email": {
            "maxLength": 255,
            "type": "string"
          },
          "password": {
            "maxLength": 100,
            "minLength": 6,
            "type": "string"
          },
          "firstName": {
            "maxLength": 100,
            "type": "string"
