          "username": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "password": {
            "type": "string"
          },
          "firstName": {
            "type": "string"
          },
          "midName": {
            "type": [
              "null",
              "string"
            ]
          },
          "lastName": {
            "type": "string"
          },
          "roleId": {
            "pattern": "^-?(?:0|[1-9]\\d*)$",
            "type": [
